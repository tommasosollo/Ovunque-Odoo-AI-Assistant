<odoo>
    <record id="view_duplicate_checker_wizard" model="ir.ui.view">
        <field name="name">ai.duplicate.checker.wizard.form</field>
        <field name="model">ai.duplicate.checker.wizard</field>
        <field name="arch" type="xml">
            <form string="Possible Duplicates">
                <group>
                    <field name="suggestions"/>
                </group>
                <footer>
                    <button name="action_apply" type="object" string="Apply" class="btn-primary"/>
                    <button string="Close" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
